{
  "title": "Redis Rust - Performance Dashboard",
  "description": "Real-time monitoring of Redis Rust server performance",
  "widgets": [
    {
      "definition": {
        "title": "Command Throughput",
        "type": "timeseries",
        "requests": [
          {
            "q": "sum:redis_rust.command.count{*} by {command}.as_rate()",
            "display_type": "bars",
            "style": {
              "palette": "cool"
            }
          }
        ]
      },
      "layout": {
        "x": 0,
        "y": 0,
        "width": 6,
        "height": 3
      }
    },
    {
      "definition": {
        "title": "Command Latency (P50/P95/P99)",
        "type": "timeseries",
        "requests": [
          {
            "q": "p50:redis_rust.command.duration{*}",
            "display_type": "line",
            "style": {
              "palette": "green"
            }
          },
          {
            "q": "p95:redis_rust.command.duration{*}",
            "display_type": "line",
            "style": {
              "palette": "orange"
            }
          },
          {
            "q": "p99:redis_rust.command.duration{*}",
            "display_type": "line",
            "style": {
              "palette": "red"
            }
          }
        ],
        "yaxis": {
          "label": "ms"
        }
      },
      "layout": {
        "x": 6,
        "y": 0,
        "width": 6,
        "height": 3
      }
    },
    {
      "definition": {
        "title": "Active Connections",
        "type": "query_value",
        "requests": [
          {
            "q": "sum:redis_rust.connections.active{*}",
            "aggregator": "last"
          }
        ],
        "precision": 0
      },
      "layout": {
        "x": 0,
        "y": 3,
        "width": 2,
        "height": 2
      }
    },
    {
      "definition": {
        "title": "Ops/sec",
        "type": "query_value",
        "requests": [
          {
            "q": "sum:redis_rust.command.count{*}.as_rate()",
            "aggregator": "avg"
          }
        ],
        "precision": 0
      },
      "layout": {
        "x": 2,
        "y": 3,
        "width": 2,
        "height": 2
      }
    },
    {
      "definition": {
        "title": "Error Rate (%)",
        "type": "query_value",
        "requests": [
          {
            "q": "100 * sum:redis_rust.command.count{status:error}.as_rate() / sum:redis_rust.command.count{*}.as_rate()",
            "aggregator": "avg"
          }
        ],
        "precision": 2,
        "conditional_formats": [
          {
            "comparator": ">",
            "value": 1,
            "palette": "red_on_white"
          },
          {
            "comparator": ">",
            "value": 0.1,
            "palette": "yellow_on_white"
          },
          {
            "comparator": "<=",
            "value": 0.1,
            "palette": "green_on_white"
          }
        ]
      },
      "layout": {
        "x": 4,
        "y": 3,
        "width": 2,
        "height": 2
      }
    },
    {
      "definition": {
        "title": "Avg Latency (ms)",
        "type": "query_value",
        "requests": [
          {
            "q": "avg:redis_rust.command.duration{*}",
            "aggregator": "avg"
          }
        ],
        "precision": 2,
        "conditional_formats": [
          {
            "comparator": ">",
            "value": 5,
            "palette": "red_on_white"
          },
          {
            "comparator": ">",
            "value": 1,
            "palette": "yellow_on_white"
          },
          {
            "comparator": "<=",
            "value": 1,
            "palette": "green_on_white"
          }
        ]
      },
      "layout": {
        "x": 6,
        "y": 3,
        "width": 2,
        "height": 2
      }
    },
    {
      "definition": {
        "title": "TTL Evictions/sec",
        "type": "query_value",
        "requests": [
          {
            "q": "sum:redis_rust.ttl.evictions{*}.as_rate()",
            "aggregator": "avg"
          }
        ],
        "precision": 0
      },
      "layout": {
        "x": 8,
        "y": 3,
        "width": 2,
        "height": 2
      }
    },
    {
      "definition": {
        "title": "Flush Duration (ms)",
        "type": "query_value",
        "requests": [
          {
            "q": "avg:redis_rust.persistence.flush.duration{*}",
            "aggregator": "avg"
          }
        ],
        "precision": 1
      },
      "layout": {
        "x": 10,
        "y": 3,
        "width": 2,
        "height": 2
      }
    },
    {
      "definition": {
        "title": "Command Breakdown by Type",
        "type": "toplist",
        "requests": [
          {
            "q": "top(sum:redis_rust.command.count{*} by {command}.as_count(), 10, 'sum', 'desc')"
          }
        ]
      },
      "layout": {
        "x": 0,
        "y": 5,
        "width": 4,
        "height": 3
      }
    },
    {
      "definition": {
        "title": "Latency by Command",
        "type": "toplist",
        "requests": [
          {
            "q": "top(avg:redis_rust.command.duration{*} by {command}, 10, 'mean', 'desc')"
          }
        ]
      },
      "layout": {
        "x": 4,
        "y": 5,
        "width": 4,
        "height": 3
      }
    },
    {
      "definition": {
        "title": "Connection Events",
        "type": "timeseries",
        "requests": [
          {
            "q": "sum:redis_rust.connections.established{*}.as_rate()",
            "display_type": "line",
            "style": {
              "palette": "green"
            }
          },
          {
            "q": "sum:redis_rust.connections.closed{*}.as_rate()",
            "display_type": "line",
            "style": {
              "palette": "red"
            }
          }
        ]
      },
      "layout": {
        "x": 8,
        "y": 5,
        "width": 4,
        "height": 3
      }
    },
    {
      "definition": {
        "title": "Shard Latency Distribution",
        "type": "heatmap",
        "requests": [
          {
            "q": "avg:redis_rust.shard.operation.duration{*} by {shard_id}"
          }
        ]
      },
      "layout": {
        "x": 0,
        "y": 8,
        "width": 6,
        "height": 3
      }
    },
    {
      "definition": {
        "title": "Persistence Metrics",
        "type": "timeseries",
        "requests": [
          {
            "q": "avg:redis_rust.persistence.flush.duration{*}",
            "display_type": "line",
            "style": {
              "palette": "blue"
            }
          },
          {
            "q": "sum:redis_rust.persistence.flush.bytes{*}.as_rate()",
            "display_type": "line",
            "style": {
              "palette": "purple"
            }
          }
        ],
        "yaxis": {
          "label": "ms / bytes"
        }
      },
      "layout": {
        "x": 6,
        "y": 8,
        "width": 6,
        "height": 3
      }
    },
    {
      "definition": {
        "title": "Service Traces",
        "type": "trace_service",
        "env": "$env",
        "service": "redis-rust",
        "span_name": "redis.command",
        "show_hits": true,
        "show_errors": true,
        "show_latency": true,
        "show_breakdown": true,
        "show_distribution": true,
        "show_resource_list": true,
        "size_format": "medium",
        "display_format": "three_column"
      },
      "layout": {
        "x": 0,
        "y": 11,
        "width": 12,
        "height": 4
      }
    }
  ],
  "template_variables": [
    {
      "name": "env",
      "default": "*",
      "prefix": "env"
    },
    {
      "name": "service",
      "default": "redis-rust",
      "prefix": "service"
    }
  ],
  "layout_type": "ordered",
  "notify_list": [],
  "reflow_type": "fixed",
  "tags": [
    "service:redis-rust",
    "team:platform"
  ]
}
